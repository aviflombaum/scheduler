<div class="row">
  <div id="cohort-index-title">
    <h1>Cohorts</h1>  
    <%= link_to "add cohort", new_cohort_path, class: 'btn btn-primary', id: 'add-cohort' %></h1>
  </div>
  <div class="cohort-list col-md-4 col-md-offset-4">
    <ul id="cohort-index">
      <% @cohorts.each do |c| %>
        <li class="" id="cohort-<%= c.id %>">
         <%= render partial: 'cohorts/user_cohort_button', locals: {cohort: c} %>
        </li>
      <% end  %>
    </ul>
  </div>
</div>